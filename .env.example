PORT=4000
NODE_ENV=development
MONGO_URI=mongodb+srv://your_username:your_password@cluster.mongodb.net
JWT_SECRET=your_jwt_secret_key
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_gmail_app_password
SENDER_EMAIL=your_email@gmail.com
CLIENT_URL=http://localhost:3000

# ============ FEEDBACK PIPELINE ============
# Redis (BullMQ) - Start with: docker run -d -p 6379:6379 redis:alpine
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# MongoDB Database for feedback (uses existing MONGO_URI)
MONGO_DB=feedback_pipeline

# AI Inference API (Hugging Face)
# Get free API key at: https://huggingface.co/settings/tokens
AI_API_KEY=your_huggingface_api_key

# Worker settings
WORKER_CONCURRENCY=5
JOB_TIMEOUT_MS=30000
MAX_RETRIES=3

# Feedback Pipeline Ports
API_PORT=3005
WORKER_METRICS_PORT=3006
